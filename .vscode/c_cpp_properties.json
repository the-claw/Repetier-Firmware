{
    "configurations": [
        {
            "name": "Arduino",
            "defines": [
                "F_CPU=16000000L",
                "ARDUINO=10809",
                "ARDUINO_AVR_SANGUINO",
                "ARDUINO_ARCH_AVR"
            ],
            "includePath": [
                "${config:arduino.path}/hardware/arduino/avr/cores/arduino",
                "${config:arduino.path}/hardware/tools/avr/avr/include",
                "${config:arduino.path}/hardware/arduino/avr/libraries/**",
                "${env:HOME}/.arduino15/packages/Sanguino/hardware/avr/1.0.3/variants/sanguino",
                "${workspaceRoot}"
            ],
            "compilerPath": "${env:HOME}/.arduino15/packages/arduino/tools/avr-gcc/5.4.0-atmel3.6.1-arduino2/bin/avr-g++",
            "compilerArgs": ["-Os", "-std=gnu++11", "-fpermissive", "-fno-exceptions", "-ffunction-sections", "-fdata-sections", "-fno-threadsafe-statics", "-Wno-error=narrowing", "-MMD", "-flto", "-mmcu=atmega1284p" ],
            "browse": {
                "limitSymbolsToIncludedHeaders": false,
                "path": [
                    "${config:arduino.path}/hardware/arduino/avr/cores/arduino",
                    "${config:arduino.path}/hardware/tools/avr/avr/include",
                    "${config:arduino.path}/hardware/arduino/avr/libraries/**",
                    "${env:HOME}/.arduino15/packages/Sanguino/hardware/avr/1.0.3/variants/sanguino",
                    "${workspaceRoot}"
                ]
            },
            "intelliSenseMode": "gcc-x86"
        }
    ],
    "version": 4
}